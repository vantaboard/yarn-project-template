    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.2",
    "mini-css-extract-plugin": "^2.4.2",
    "msw": "^0.35.0",
    "nanoid": "^3.1.29",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "prism-react-renderer": "^1.2.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-error-boundary": "^3.1.3",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.17.2",
    "react-query": "^3.26.0",
